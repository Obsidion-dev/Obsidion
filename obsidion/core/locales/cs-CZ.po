msgstr "Zobrazí dobu činnosti objektu."

#: obsidion/core/core_commands.py:111
msgid "Less than one second"
msgstr "Méně než jedna sekunda"

#: obsidion/core/core_commands.py:113
msgid "Been up for: **{time_quantity}** (since {timestamp} UTC)"
msgstr "Bylo spuštěno pro: **{time_quantity}** (od {timestamp} UTC)"

#: obsidion/core/core_commands.py:126
#, docstring
msgid "Invite the bot to your server."
msgstr "Pozvěte na svůj server robot."

#: obsidion/core/core_commands.py:128
msgid "You can invite {name} to your Discord server by [clicking here]({invite})."
msgstr "Do vašeho serveru Discord můžete pozvat {name} klepnutím sem ({invite})."

#: obsidion/core/core_commands.py:147
#, docstring
msgid "Leaves the current server."
msgstr "Opustí aktuální server."

#: obsidion/core/core_commands.py:149
msgid "Are you sure you want me to leave the current server? (yes/no)"
msgstr "Jste si jisti, že chcete, abych opustil aktuální server? (ano/ne)"

#: obsidion/core/core_commands.py:158
#, docstring
